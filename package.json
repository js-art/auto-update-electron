{
  "name": "auto-update-electron",
  "version": "1.0.3",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron . ",
    "package": "electron-builder -p always --win",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "build": {
    "publish": [
      {
        "provider": "github",
        "owner": "js-art",
        "repo": "auto-update-electron",
        "private":false
      }
    ],
    "win":{
      "target":"nsis"
    },
    "nsis":{
      "deleteAppDataOnUninstall":true,
      "oneClick":false
    }
  },
  
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "electron-log": "^5.1.1",
    "electron-updater": "^6.1.7"
  },
  "devDependencies": {
    "electron": "^28.2.2",
    "electron-builder": "^24.9.1"
  }
}
